{% extends 'base.html.twig' %}

{% block title %}RepportLogs{% endblock %}

{% block body %}
    <h1>RepportLogs</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ repport_log.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ repport_log.date ? repport_log.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Modified_field</th>
                <td>{{ repport_log.modifiedField }}</td>
            </tr>
            <tr>
                <th>New_value</th>
                <td>{{ repport_log.newValue }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_repport_logs_index') }}">back to list</a>

    <a href="{{ path('app_repport_logs_edit', {'id': repport_log.id}) }}">edit</a>

    {{ include('repport_logs/_delete_form.html.twig') }}
{% endblock %}
